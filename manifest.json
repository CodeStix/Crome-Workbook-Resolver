{
  "manifest_version": 2,
  "name": "Knooppunt-Zonder-Punt",
  "description": "Makkelijk gelekte oplossingen weergeven van verschillende schoolboeken op knooppunt.net en scoodle.be. ",
  "version": "0.2.0",
  "content_scripts": [
    {
      "matches": ["https://cds.knooppunt.net/Pages/*", "http://cds.knooppunt.net/Pages/*", "https://content.plantyn.com/Pages/*", "http://content.plantyn.com/Pages/*"],
      "js": ["resolve.js", "image2base64.js", "jspdf.min.js"]
    }
  ],
  "web_accessible_resources": [
    "leaks.json"
  ],
  "permissions": [
    "storage"
  ]
}